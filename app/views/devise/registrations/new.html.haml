= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
  #error_explanation
    = devise_error_messages!
  %p
    .prompt.field= f.label :email
    =f.text_field :email
  %p
    .prompt.field= f.label :password
    = f.password_field :password
  %p
    .prompt.field= f.label :password_confirmation
    = f.password_field :password_confirmation
  %p
    = f.submit t(:sign_up)
.action= render :partial => 'devise/shared/links'
